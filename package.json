{
  "name": "family-finances",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "expo prebuild",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && rm -rf Pods && rm -rf Podfile.lock && pod cache clean --all && pod deintegrate && pod setup && cd ..",
    "android:build:apk": "cd android && ./gradlew assembleRelease && cd ..",
    "android:build:aab": "cd android && ./gradlew bundleRelease && cd ..",
    "android:build:apk:install": "cd android && ./gradlew installRelease && cd ..",
    "android:build:aab:install": "cd android && ./gradlew installRelease && cd ..",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:dev": "eas build -p android --profile development",
    "build:apk": "eas build -p android --profile preview",
    "build:store": "eas build -p android --profile production",
    "start:emulator": "emulator -avd Pixel_2_API_33"
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.3",
    "@nozbe/watermelondb": "^0.26.0",
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/datetimepicker": "6.7.3",
    "@react-native-community/hooks": "^3.0.0",
    "@react-native-picker/picker": "2.4.8",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.7",
    "expo": "~48.0.18",
    "expo-asset": "^8.9.1",
    "expo-build-properties": "~0.6.0",
    "expo-dev-client": "~2.2.1",
    "expo-document-picker": "~11.2.2",
    "expo-file-system": "~15.2.2",
    "expo-media-library": "^15.2.3",
    "expo-sharing": "^11.2.2",
    "expo-splash-screen": "~0.18.2",
    "expo-status-bar": "~1.4.4",
    "formik": "^2.2.9",
    "native-base": "^3.4.28",
    "openai": "^3.3.0",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-chart-kit": "^6.12.0",
    "react-native-csv": "^0.2.0",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-loading-dots": "^1.3.2",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "4.5.0",
    "react-native-svg": "13.4.0",
    "react-native-url-polyfill": "^1.3.0",
    "xlsx": "^0.18.5",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-decorators": "^7.22.7"
  },
  "private": true
}
